@{
  bool userAuth = (User?.Identity?.IsAuthenticated ?? false);
  PdpSiteRazorModel psr = Model.PSR;
  // QebUserRestContext qurc = Model.QURC;
}

@if (!userAuth)
{
  <p>
    CoreWebLib menus only visible in authorized mode. You may   
    <a asp-page="/NPDS/AnonMode/LoginUser">login</a> or
    <a asp-page="/NPDS/AnonMode/RegisterUser">register</a>.
  </p>
}

@if (userAuth && PDPSS.AppUseDevtestFeature)
{
  @Html.Raw(psr.FormatBodyTitle("CoreWebLib Project Menus"))
  <partial name="_CoreWebLibListPageMenu" />  
}
