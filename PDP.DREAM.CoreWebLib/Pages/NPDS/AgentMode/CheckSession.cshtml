@page
@attribute [PdpRazorPageRoute(DepCoreWebLib, DepAgentModeCheckSession)]
@model AgentModeCheckSession
@{
  bool userAuth = (User?.Identity?.IsAuthenticated ?? false);
  PdpSiteRazorModel psr = Model.PSR;
  QebUserRestContext qurc = Model.QURC;
}
@Html.Raw(psr.FormatBodyTitle())
<p><partial name="@psr.RazorBodyMenu" /></p>

@if (userAuth && PDPSS.AppUseDevtestFeature)
{
  <h5>AspNetCore ViewData Test</h5>

  <p>Guids from webapp cookies, keys from request querystring.</p>
  <p>User: Guid = @qurc.QebUserGuid, Key = @qurc.QebUserKey</p>
  <p>Agent: Guid = @qurc.QebAgentGuid, Key = @qurc.QebAgentKey</p>
  <p>Session: Guid = @qurc.QebSessionGuid, Key = @qurc.QebSessionKey</p>
}
