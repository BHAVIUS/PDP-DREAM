@page
@attribute [PdpRazorPageRoute(DepCoreWebLib, DepCwlHomeSiteSettings)]
@model CwlHomeSiteSettings
@{
  var psr = (PdpSiteRazorModel)Model.PSR;
  var propList = PDPSS.GetType().GetProperties().OrderBy(p => p.Name);
}
@Html.Raw(psr.FormatBodyTitle())


@if (!string.IsNullOrEmpty(psr.RazorBodyMenu))
{
  <p><partial name="@psr.RazorBodyMenu" /></p>
}
@Html.Raw(psr.FormatBodyTitle())

<table class="pdpTable">
  <tr><td>Name</td><td>Value</td><td>Type</td></tr>
  @foreach (PropertyInfo item in propList)
  {
    <tr><td>@item?.Name</td><td>@item?.GetValue(PDPSS)?.ToString()</td><td>@item?.PropertyType</td></tr>
  }
</table>
