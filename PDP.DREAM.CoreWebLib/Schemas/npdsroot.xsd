			<extension base="npds:stSupportingTag">
				<attributeGroup ref="npds:agFgroupChild"/>
			</extension>
		</simpleContent>
	</complexType>
	<complexType name="ctSupportingTagSet">
		<sequence>
			<element name="SupportingTag" type="npds:ctSupportingTag" minOccurs="0" maxOccurs="unbounded"/>
		</sequence>
		<attributeGroup ref="npds:agFgroupParent"/>
	</complexType>
	<simpleType name="stSupportingLabel">
		<restriction base="anyURI">
			<minLength value="8"/>
			<maxLength value="384"/>
		</restriction>
	</simpleType>
	<complexType name="ctSupportingLabel">
		<simpleContent>
			<extension base="npds:stSupportingLabel">
				<attributeGroup ref="npds:agFgroupChild"/>
			</extension>
		</simpleContent>
	</complexType>
	<complexType name="ctSupportingLabelSet">
		<sequence>
			<element name="SupportingLabel" type="npds:ctSupportingLabel" minOccurs="0" maxOccurs="unbounded"/>
		</sequence>
		<attributeGroup ref="npds:agFgroupParent"/>
	</complexType>
	<complexType name="ctCrossReference">
		<simpleContent>
			<extension base="token">
				<attributeGroup ref="npds:agFgroupChild"/>
			</extension>
		</simpleContent>
	</complexType>
	<complexType name="ctCrossReferenceSet">
