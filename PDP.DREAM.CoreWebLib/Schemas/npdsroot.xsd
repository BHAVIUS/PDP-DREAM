	<annotation>
		<documentation> Global attributes for groups and elements; note problem with Altova
			validator which requires use of namespace qualifier on global attributes when compared
			with defining attributes locally for each complex type where the latter (defining
			attributes locally) is used in this schema in order to avoid the problem; however even
			though attributes are defined and used locally in each complex type, they are also
			collected and defined globally here as well for convenience; the "ref" attribute may be
			used in a complex type to reference them.
			http://stackoverflow.com/questions/3469997/xml-schema-attribute-ref In XML Schemas all
			global element, attribute or type definitions must be qualified. Thus all globally
			defined attributes will be namespace prefixed even if you have defined
			attributeFormDefault as "unqualified". Workaround is to define this attribute inside a
			attribute group or a global type and then you can reference to this named group or to
			extend this named type. When you need this attribute, refer to the attributeGroup
			instead of the attribute. Attribute group doesn't need to contain all attributes the
			element uses, this should also be valid.</documentation>
	</annotation>
	<attributeGroup name="agFgroupParent">
		<attribute name="Count" type="int" use="optional" default="0"/>
		<anyAttribute namespace="##any" processContents="lax"/>
	</attributeGroup>
	<attributeGroup name="agFgroupChild">
		<attribute name="IsMarked" type="boolean" use="optional" default="false"/>
		<attribute name="IsPrivate" type="boolean" use="optional" default="false"/>
		<attribute name="IsPrincipal" type="boolean" use="optional" default="false"/>
		<attribute name="Index" type="unsignedByte" use="optional" default="0"/>
		<attribute name="Format" type="npds:etFormat" use="optional" default="none"/>
		<attribute name="Priority" type="unsignedByte" use="optional" default="0"/>
		<anyAttribute namespace="##any" processContents="lax"/>
	</attributeGroup>
	<attributeGroup name="agEntityLabel">
		<attributeGroup ref="npds:agFgroupChild"/>
		<attribute name="EntityType" type="npds:etEntityType" use="optional" default="Untyped"/>
		<attribute name="IsGenerating" type="boolean" use="optional" default="false"/>
		<attribute name="IsResolvable" type="boolean" use="optional" default="false"/>
		<anyAttribute namespace="##any" processContents="lax"/>
	</attributeGroup>
	<attributeGroup name="agRecordProvenance">
		<attributeGroup ref="npds:agFgroupChild"/>
		<attribute name="IsCached" type="boolean" use="optional" default="false"/>
	</attributeGroup>
	<attributeGroup name="agRecordDistribution">
		<attributeGroup ref="npds:agFgroupChild"/>
		<attribute name="IsConcise" type="boolean" use="optional" default="false"/>
		<attribute name="TimeToLiveUnit" type="npds:stNvarchar32" use="optional" default="day"/>
		<attribute name="TimeToLiveValue" type="npds:stInt" use="optional" default="0"/>
	</attributeGroup>
	<annotation>
		<documentation>Globally unique identifier (per SQL definition) for the resource infoset
			assembled from distributed records at PORTAL and DOORS servers; this optional identifier
			is distinct from the EntityLabel as the required globally unique identifer for the
			resource entity. </documentation>
	</annotation>
	<attributeGroup name="agScribeParent">
		<attribute name="IsAuthorPrivate" type="boolean" use="optional" default="false"/>
		<attribute name="IsAgentShared" type="boolean" use="optional" default="false"/>
		<attribute name="IsUpdaterLimited" type="boolean" use="optional" default="false"/>
		<attribute name="IsManagerReleased" type="boolean" use="optional" default="false"/>
		<anyAttribute namespace="##any" processContents="lax"/>
	</attributeGroup>
	<attributeGroup name="agScribeChild">
		<attribute name="Guid" type="npds:stUniqueidentifier" use="optional" default="00000000-0000-0000-0000-000000000000"/>
		<attribute name="Handle" type="npds:stNvarchar32" use="optional"/>
		<anyAttribute namespace="##any" processContents="lax"/>
	</attributeGroup>
	<annotation>
		<documentation> Complex Types </documentation>
	</annotation>
	<complexType name="ctName">
		<annotation>
			<documentation>Name of resource entity for display in applications: usually the name of
				the person, organization, physical offline or virtual online entity with desired
				spelling case and punctuation as used in the official name for the resource
				entity</documentation>
		</annotation>
		<simpleContent>
			<extension base="npds:stNvarchar128">
				<anyAttribute namespace="##any" processContents="lax"/>
			</extension>
		</simpleContent>
	</complexType>
	<complexType name="ctNature">
		<annotation>
			<documentation>Nature of resource entity for display in applications: usually a natural
				language phrase concisely describing the resource entity.</documentation>
		</annotation>
		<simpleContent>
			<extension base="npds:stNvarchar256">
				<anyAttribute namespace="##any" processContents="lax"/>
			</extension>
		</simpleContent>
	</complexType>
	<complexType name="ctRegisteredUri">
		<annotation>
			<documentation>complexType for use by RegisteredUri in any EntityLabel; ctRegisteredUri
				is an extension of stUri.</documentation>
		</annotation>
		<simpleContent>
			<extension base="npds:stUri">
				<attributeGroup ref="npds:agFgroupChild"/>
			</extension>
		</simpleContent>
	</complexType>
	<complexType name="ctEntityLabel">
		<annotation>
			<documentation>complexType for the required canonical label serving as the globally
				unique identifier for the resource entity</documentation>
		</annotation>
		<simpleContent>
			<extension base="npds:ctRegisteredUri">
				<attributeGroup ref="npds:agEntityLabel"/>
			</extension>
		</simpleContent>
	</complexType>
	<complexType name="ctEntityAliasLabelSet">
		<annotation>
			<documentation>complexType for the permitted alias labels that may also be used to
				identify the resource entity; all labels, whether canonical or alias, must be
				globally unique throughout the entire PORTAL-DOORS System</documentation>
		</annotation>
		<sequence>
			<element name="AliasLabel" type="npds:ctEntityLabel" minOccurs="0" maxOccurs="unbounded"/>
		</sequence>
		<attributeGroup ref="npds:agFgroupParent"/>
	</complexType>
	<complexType name="ctPersonLabel">
		<simpleContent>
			<restriction base="npds:ctEntityLabel">
				<attribute name="EntityType" type="npds:etEntityType" use="optional" fixed="Person"/>
			</restriction>
		</simpleContent>
	</complexType>
	<complexType name="ctOrganizationLabel">
		<simpleContent>
			<restriction base="npds:ctEntityLabel">
				<attribute name="EntityType" type="npds:etEntityType" use="optional" fixed="Organization"/>
			</restriction>
		</simpleContent>
	</complexType>
	<complexType name="ctOtherEntityLabel">
		<simpleContent>
			<restriction base="npds:ctEntityLabel">
				<attribute name="EntityType" type="npds:etEntityType" use="optional" default="Untyped"/>
			</restriction>
		</simpleContent>
	</complexType>
	<complexType name="ctContactLabel">
		<simpleContent>
			<restriction base="npds:ctEntityLabel">
				<attribute name="EntityType" type="npds:etEntityType" use="optional" fixed="Person"/>
			</restriction>
		</simpleContent>
	</complexType>
	<complexType name="ctOwnerLabel">
		<simpleContent>
			<restriction base="npds:ctEntityLabel">
				<attribute name="EntityType" type="npds:etSystemConstituent" use="optional"/>
			</restriction>
		</simpleContent>
	</complexType>
	<complexType name="ctRegistrantLabel">
		<simpleContent>
			<restriction base="npds:ctEntityLabel">
				<attribute name="EntityType" type="npds:etSystemConstituent" use="optional"/>
			</restriction>
		</simpleContent>
	</complexType>
	<complexType name="ctRegistrarLabel">
		<simpleContent>
			<restriction base="npds:ctEntityLabel">
				<attribute name="EntityType" type="npds:etHierarchyScribeNodes" use="optional"/>
				<attribute name="IsPrincipal" type="boolean" use="optional" default="false"/>
				<attribute name="Priority" type="unsignedByte" use="optional" default="0"/>
			</restriction>
		</simpleContent>
	</complexType>
	<complexType name="ctRegistryLabel">
		<simpleContent>
			<restriction base="npds:ctEntityLabel">
				<attribute name="EntityType" type="npds:etHierarchyPortalNodes" use="optional"/>
				<attribute name="IsPrincipal" type="boolean" use="optional" default="false"/>
				<attribute name="Priority" type="unsignedByte" use="optional" default="0"/>
			</restriction>
		</simpleContent>
	</complexType>
	<complexType name="ctDirectoryLabel">
		<simpleContent>
			<restriction base="npds:ctEntityLabel">
				<attribute name="EntityType" type="npds:etHierarchyDoorsNodes" use="optional"/>
				<attribute name="IsPrincipal" type="boolean" use="optional" default="false"/>
				<attribute name="Priority" type="unsignedByte" use="optional" default="0"/>
			</restriction>
		</simpleContent>
	</complexType>
	<complexType name="ctDiristryLabel">
		<simpleContent>
			<restriction base="npds:ctEntityLabel">
				<attribute name="EntityType" type="npds:etHierarchyNexusNodes" use="optional"/>
				<attribute name="IsPrincipal" type="boolean" use="optional" default="false"/>
				<attribute name="Priority" type="unsignedByte" use="optional" default="0"/>
			</restriction>
		</simpleContent>
	</complexType>
	<simpleType name="stPrincipalTag">
		<restriction base="NCName">
			<minLength value="1"/>
			<maxLength value="128"/>
			<pattern value=".*"/>
		</restriction>
	</simpleType>
	<simpleType name="stSupportingTag">
		<restriction base="token">
			<minLength value="1"/>
			<maxLength value="128"/>
		</restriction>
	</simpleType>
	<complexType name="ctSupportingTag">
