	<complexType name="ctDistribution">
		<complexContent>
			<extension base="npds:ctSkiptext">
				<attributeGroup ref="npds:agRecordDistribution"/>
			</extension>
		</complexContent>
	</complexType>
	<complexType name="ctDistributionSet">
		<sequence>
			<element name="Distribution" type="npds:ctDistribution" minOccurs="0" maxOccurs="unbounded"/>
