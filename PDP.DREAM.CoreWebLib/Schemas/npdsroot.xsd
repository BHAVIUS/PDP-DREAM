			<enumeration value="NexusRoot"/>
			<enumeration value="NexusSecondary"/>
			<enumeration value="NpdsDirectory"/>
			<enumeration value="NpdsDiristry"/>
			<enumeration value="NpdsRegistrar"/>
			<enumeration value="NpdsRegistry"/>
			<enumeration value="NpdsRoot"/>
			<enumeration value="OfflineRealEntity"/>
			<enumeration value="OneDimSequence"/>
			<enumeration value="OnlineVirtualEntity"/>
