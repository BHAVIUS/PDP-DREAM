	<annotation>
		<documentation> Type naming conventions for global types include the prefix "st" for
			simpleTypes, "et" for simpleTypes with enumerations, "ct" for complexTypes, "g" for
			groups, and the suffix "Set" for unbounded sequences of the underlying element type.
			These naming conventions apply only to the types to facilitate recognition of the
			distinction between the type and an instance of the type. Thus, the prefix/suffix
			conventions do not apply to elements or attributes (as instances of the types).
		</documentation>
	</annotation>
	<simpleType name="stNonzeroTinyint">
		<annotation>
			<documentation>value space of XML Schema unsignedByte is 0-255 equivalent to a SQL
				Server tinyint; the restriction excludes zero</documentation>
		</annotation>
		<restriction base="unsignedByte">
			<minInclusive value="1"/>
			<maxInclusive value="255"/>
		</restriction>
	</simpleType>
	<simpleType name="stInt">
		<annotation>
			<documentation>upper bound 2147483648 = 2^31</documentation>
		</annotation>
		<restriction base="int">
			<minInclusive value="0"/>
			<maxInclusive value="2147483647"/>
		</restriction>
	</simpleType>
	<simpleType name="stNvarchar16">
		<restriction base="token">
			<minLength value="0"/>
			<maxLength value="16"/>
		</restriction>
	</simpleType>
	<simpleType name="stNvarchar32">
		<restriction base="token">
			<minLength value="0"/>
			<maxLength value="32"/>
		</restriction>
	</simpleType>
	<simpleType name="stNvarchar64">
		<restriction base="token">
			<minLength value="0"/>
			<maxLength value="64"/>
		</restriction>
	</simpleType>
	<simpleType name="stNvarchar128">
		<restriction base="token">
			<minLength value="0"/>
			<maxLength value="128"/>
		</restriction>
	</simpleType>
	<simpleType name="stNvarchar256">
		<restriction base="token">
			<minLength value="0"/>
			<maxLength value="256"/>
		</restriction>
	</simpleType>
	<simpleType name="stNvarchar4096">
		<restriction base="token">
			<minLength value="0"/>
			<maxLength value="4096"/>
		</restriction>
	</simpleType>
	<simpleType name="stUniqueidentifier">
		<restriction base="string">
			<pattern value="([0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{12})|(\{[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{12}\})"/>
		</restriction>
	</simpleType>
	<simpleType name="stUri">
		<annotation>
			<documentation>Per RFC 3987 discussion, the xsd:anyURI type includes IRIs and IRI
				references. stUri imposes a length restriction on anyURI.</documentation>
		</annotation>
		<restriction base="anyURI">
			<minLength value="0"/>
			<maxLength value="384"/>
		</restriction>
	</simpleType>
	<annotation>
		<documentation>definitions and declarations for XML/RDF/RDFS/OWL</documentation>
	</annotation>
	<complexType name="ctSkiptext" mixed="true">
		<sequence>
			<any processContents="skip" minOccurs="0" maxOccurs="unbounded"/>
		</sequence>
		<attributeGroup ref="npds:agFgroupChild"/>
	</complexType>
	<complexType name="ctSemantictext">
		<sequence>
			<any namespace="http://www.w3.org/1999/02/22-rdf-syntax-ns# http://www.w3.org/2000/01/rdf-schema# http://www.w3.org/2002/07/owl#" processContents="strict" minOccurs="0" maxOccurs="unbounded"/>
		</sequence>
		<attributeGroup ref="npds:agFgroupChild"/>
	</complexType>
	<complexType name="ctRdfDescriptionSet">
		<annotation>
			<documentation>If used, the element RdfDescription in NPDS must be implemented as an
				alias for the element Description in RDF. An alias is used here to avoid
				difficulties caused by requiring implementation of an RDF processor and/or importing
				the RDF namespace elements.</documentation>
		</annotation>
		<sequence>
			<element name="RdfDescription" minOccurs="0" maxOccurs="unbounded"/>
		</sequence>
		<attributeGroup ref="npds:agFgroupChild"/>
	</complexType>
	<annotation>
		<documentation>global enumeration types (named with prefix convention of "et") for groups
			and elements</documentation>
	</annotation>
	<simpleType name="etFormat">
		<annotation>
			<documentation>Format of element content.</documentation>
		</annotation>
