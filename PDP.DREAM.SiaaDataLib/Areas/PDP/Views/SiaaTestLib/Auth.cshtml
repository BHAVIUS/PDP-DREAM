<h4>PDP Agent/Session</h4>
@using (Html.BeginForm("Auth", "ScribeTestApi", new { area = "PDP"}, FormMethod.Post ))
{
  <div class="form-group">
    @Html.LabelFor(m => m.AgentGuid, new { @class = "control-label col-md-2" })
    @Html.TextBoxFor(m => m.AgentGuid, new { @class = "form-control col-md-6" })
  </div>
  <div class="form-group">
    @Html.LabelFor(m => m.SessionGuid, new { @class = "control-label col-md-2" })
    @Html.TextBoxFor(m => m.SessionGuid, new { @class = "form-control col-md-6" })
  </div>
}

